{
  "name": "celf-cli",
  "version": "0.0.2",
  "description": "Cellinlab's self-developed CLI (Command-Line Interface), used for rapid development.",
  "main": "index.js",
  "bin": {
    "ccli": "./bin/ccli.js"
  },
  "scripts": {
